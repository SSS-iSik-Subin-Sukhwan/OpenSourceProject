<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myeatingmapdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".CheckRestaurant.PopUpActivity" android:theme="@android:style/Theme.Dialog" />

        <activity android:name=".CheckRestaurant.CheckRestaurantActivity" />
        <activity android:name=".FindRestaurant.FindRestaurantResultViewActivity" />
        <activity android:name=".Memo.MemoRestaurantActivity" />
        <activity android:name=".MarkRestaurant.MarkRestaurantActivity" />
        <activity android:name=".FindRestaurant.FindRestaurantMarkActivity" />
        <activity android:name=".ListView.SearchedPlaceListViewActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
    </application>

</manifest>